input {
	file {
		 path => "file.csv"
		 start_position => "beginning" 
		 sincedb_path => "/dev/null" 
	}
}

filter {
	 csv { 
		columns => [name,location,size,cost,appendID]
		separator=> ","
	}
}

output {
	 elasticsearch {
		 hosts => ["localhost:9200"] 
		 document_id => "%{appendID}" 
		 index => "file.csv.john.loverich@gmail.com"
	}
}

